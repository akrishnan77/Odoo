{
  "expo": {
    "name": "ConnectIQ",
    "slug": "connectiq-mobile",
    "scheme": "connectiq",
    "version": "0.1.0",   
    "orientation": "portrait",
    "icon": "",
    "splash": {
      "image": "",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    
    "assetBundlePatterns": [
      "**/*"
    ],

  "ios": { "supportsTablet": true, "bundleIdentifier": "com.connectiq.app" },
    "android": {
      "package": "com.connectiq.app",
      "adaptiveIcon": { "foregroundImage": "", "backgroundColor": "#ffffff" },
      "intentFilters": [
        {
          "action": "VIEW",
          "category": ["BROWSABLE", "DEFAULT"],
          "data": [{ "scheme": "connectiq", "host": "auth" }]
        }
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "msClientId": "f6a9843a-5e99-41a5-88f1-34466204cb13",
      "tenant": "e4e92fef-2d65-4067-b53d-79120c33e2f3",
      "scopes": ["User.Read"],
      "redirectScheme": "connectiq",
  "redirectPath": "auth"  
    }
  }
}
